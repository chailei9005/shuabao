public class RegexConstants {
    //手机号码验证
    public static final String REG_PHONE_CHINA = "^(13[0-9]|14[5|7]|15[0|1|2|3|5|6|7|8|9]|18[0|1|2|3|5|6|7|8|9])\\d{8}$";
    //邮箱验证
    public static final String REG_EMAIL = "\\w+([-+.]\\w+)*@\\w+([-.]\\w+)*\\.\\w+([-.]\\w+)*";
    //是否数字
    public static final String REG_NUMBER = "^[0-9]*$";
    //英文和数字
    public static final String REG_NUMBER_ENGLISH = "^[A-Za-z0-9]+$";
    //英文数字下划线
    public static final String REG_HUMBER_ENGLISH_UNDERLINE = "^\\w{3,20}$";
    //国内电话号码
    public static final String REG_CHINA_PHONE = "\\d{3}-\\d{8}|\\d{4}-\\d{7}";
    //身份证号码
    public static final String REG_IDCARDNUMBER = "^\\d{8,18}|[0-9x]{8,18}|[0-9X]{8,18}?$";
}
